name: SSID Policy
desc: 自用SSID分流策略，根据网络变化全自动切换策略。

proxy-groups:
  - name: 🚥 节点选择
    type: select
    proxies:
      - 🚀 节点选择
      - DIRECT
    ssid-policy:
      yorun-PC: DIRECT
      default: 🚀 节点选择

  - name: 🚥 Netflix
    type: select
    proxies:
      - 🎥 Netflix
      - DIRECT
    ssid-policy:
      yorun-PC: DIRECT
      default: 🎥 Netflix

  - name: 🚥 漏网之鱼
    type: select
    proxies:
      - 🐟 漏网之鱼
      - DIRECT
    ssid-policy:
      yorun-PC: DIRECT
      default: 🐟 漏网之鱼
